    "version": "0.2.0",
    "configurations": [
        {
            "name": "go debug flagsarguments",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "env": {},
            "args": [
                "-age=37",
                "-married=false",
                "-name=Sherman"
            ]
        },
        {
            "name": "go run flagsarguments",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "${workspaceFolder}\\bin\\${workspaceFolderBasename}.exe",
            "env": {},
            "args": [
                "-age=37",
                "-married=false",
                "-name=Sherman"
            ]
        }
    ]
}